    "i18next-icu": "^2.4.3",
    "immer": "^11.1.4",
    "intl-messageformat": "^11.1.2",
    "nosleep.js": "^0.12.0",
    "o9n": "^2.1.1",
    "react-infinite-scroll-component": "^6.1.1",
    "react-merge-refs": "^3.0.2",
